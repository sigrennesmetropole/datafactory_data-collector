{
  "name": "http-reaper",
  "version": "0.6.0",
  "description": "Downloads the content of the given URL and put the payload into 'successTube' or 'errorTube' when it fails",
  "repository": "https://github.com/maxleiko/tpl-node-app",
  "author": "Maxime Tricoire <max.tricoire@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "mocha",
    "generate": "pegjs --format commonjs src/csv-types/grammar.pegjs",
    "compile": "tsc -b src",
    "build": "yarn clean && yarn compile",
    "bundle": "nexe dist/cli.js --build -t alpine-x64-12.9.1 -n http-reaper",
    "prepackage": "yarn test && yarn build",
    "package": "yarn bundle",
    "prestart": "yarn compile",
    "debug": "node --inspect-brk dist/cli.js",
    "start": "node dist/cli.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/debug": "^4.1.5",
    "@types/jsftp": "^2.1.0",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.23",
    "@types/redis": "^2.8.27",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "mocha": "^8.1.1",
    "nexe": "^3.3.7",
    "nock": "^13.0.4",
    "pegjs": "^0.10.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@beanstalk/core": "^0.2.2",
    "@fast-csv/parse": "^4.3.1",
    "@types/ssh2-sftp-client": "^7.0.1",
    "arg": "^4.1.3",
    "chardet": "^1.4.0",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "got": "11.8.5",
    "iconv-lite": "^0.6.3",
    "jsftp": "^2.1.3",
    "micromatch": "^4.0.2",
    "redis": "^3.0.2",
    "ssh2-sftp-client": "^8.0.0"
  }
}