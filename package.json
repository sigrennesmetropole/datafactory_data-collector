{
  "name": "http-reaper",
  "version": "0.6.0",
  "description": "Downloads the content of the given URL and put the payload into 'successTube' or 'errorTube' when it fails",
  "repository": "https://github.com/maxleiko/tpl-node-app",
  "author": "Maxime Tricoire <max.tricoire@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "mocha",
    "generate": "pegjs --format commonjs src/csv-types/grammar.pegjs",
    "compile": "tsc -b src",
    "build": "yarn clean && yarn compile",
    "bundle": "nexe dist/cli.js --build -t alpine-x64-12.9.1 -n http-reaper",
    "prepackage": "yarn test && yarn build",
    "package": "yarn bundle",
    "prestart": "yarn compile",
    "debug": "node --inspect-brk dist/cli.js",
    "start": "node dist/cli.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/debug": "^4.1.5",
    "@types/jsftp": "^2.1.0",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "8.2.3",
    "@types/node": "14.18.36",
    "@types/redis": "2.8.32",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "chai": "^4.2.0",
    "eslint": "7.32.0",
    "mocha": "8.4.0",
    "nexe": "^3.3.7",
    "nock": "^13.0.4",
    "pegjs": "^0.10.0",
    "ts-node": "9.1.1",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@beanstalk/core": "0.2.2",
    "@fast-csv/parse": "4.3.6",
    "@types/ssh2-sftp-client": "7.1.0",
    "arg": "4.1.3",
    "chardet": "1.4.0",
    "debug": "4.3.4",
    "dotenv": "8.6.0",
    "got": "11.8.5",
    "iconv-lite": "^0.6.3",
    "jsftp": "^2.1.3",
    "micromatch": "^4.0.2",
    "redis": "3.1.2",
    "ssh2-sftp-client": "8.0.0"
  }
}